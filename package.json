{
  "name": "template",
  "version": "0.0.1",
  "description": "A Node Typescript template",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/index.js",
    "build": "rimraf ./dist && tsc --build",
    "lint": "eslint src",
    "tsnode": "ts-node ./src/index.ts",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier --write src/**/*.{js,ts,json}"
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "keywords": [
    "Husky",
    "Eslint",
    "Typescript",
    "Nodejs",
    "Nodemon"
  ],
  "author": "ChampionBuffalo1 <primarymail@duck.com>",
  "license": "MIT",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "devDependencies": {
    "@types/node": "^18.8.5",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-class-members": "^1.15.2",
    "husky": "^8.0.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "ioredis": "^5.2.3",
    "mongoose": "^6.11.3"
  }
}
